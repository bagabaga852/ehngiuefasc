BOSSTP = Library:Tab("魔王TP")


if First_Sea then

BOSSTP:Seperator("魔王傳送點--(一海)[Boss TP -- First Sea]")
BOSSTP:Button("獸人領主(Orc Lord)", function()
    local Sound = Instance.new("Sound",game:GetService("SoundService"))
    Sound.SoundId = "rbxassetid://9125530803"
    Sound:Play()
local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
if game:GetService("ReplicatedStorage").MOB:FindFirstChild("Monster [Lv. 2500]")  then

    plr.CFrame = game:GetService("ReplicatedStorage").MOB["Monster [Lv. 2500]"].HumanoidRootPart.CFrame * CFrame.new(0,500,0)
    print("Replicated")
elseif game:GetService("Workspace").Monster.Boss:FindFirstChild("Monster [Lv. 2500]") then
    
    plr.CFrame = game:GetService("Workspace").Monster.Boss["Monster [Lv. 2500]"].HumanoidRootPart.CFrame * CFrame.new(0,500,0)
    print("Workspace")
end
end)

BOSSTP:Button("紅髮(Expert Swordman)", function()
    local Sound = Instance.new("Sound",game:GetService("SoundService"))
    Sound.SoundId = "rbxassetid://9125530803"
    Sound:Play()
    local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
    if game:GetService("ReplicatedStorage").MOB:FindFirstChild("Expert Swordman [Lv. 3000]")  then
    
        plr.CFrame = game:GetService("ReplicatedStorage").MOB["Expert Swordman [Lv. 3000]"].HumanoidRootPart.CFrame * CFrame.new(0,60,0)
        print("Replicated")
    elseif game:GetService("Workspace").Monster.Boss:FindFirstChild("Expert Swordman [Lv. 3000]") then
        
        plr.CFrame = game:GetService("Workspace").Monster.Boss["Expert Swordman [Lv. 3000]"].HumanoidRootPart.CFrame * CFrame.new(0,60,0)
        print("Workspace")
    end
    end)

    BOSSTP:Button("回到重生點(Starting Point)", function()
    local Sound = Instance.new("Sound",game:GetService("SoundService"))
    Sound.SoundId = "rbxassetid://9125530803"
    Sound:Play()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace:FindFirstChild("SpawnLocation").CFrame
end)

else

    BOSSTP:Seperator("魔王傳送點--(二海)[Boss TP -- Second Sea]")

BOSSTP:Button("大媽(Ms. Mother)", function()
local Sound = Instance.new("Sound",game:GetService("SoundService"))
    Sound.SoundId = "rbxassetid://9125530803"
    Sound:Play()
local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
if game:GetService("ReplicatedStorage").MOB:FindFirstChild("Ms. Mother [Lv. 7500]") then

